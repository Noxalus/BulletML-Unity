<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "bulletml.dtd">
<bulletml>
  <action label="top">
    <repeat>
      <times>0</times>
      <action>
        <repeat>
          <times>10</times>
          <action>
            <fire>
              <bullet>
                <direction type="absolute">$rand * 360</direction>
                <speed>0.5</speed>
              </bullet>
            </fire>
            <wait>0</wait>
          </action>
        </repeat>
        <wait>1</wait>
      </action>
    </repeat>
  </action>
</bulletml>